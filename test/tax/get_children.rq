# @endpoint test
# @param taxid=40674
SELECT ?order ?order_name ?family ?family_name ?species ?species_name
WHERE {
  ?species rdfs:subClassOf+ taxid:{{taxid}} .
  ?species rdfs:label ?species_name .
  ?species taxon:rank taxon:Species .
  ?species rdfs:subClassOf+ ?family .
  ?family taxon:rank taxon:Family .
  ?family rdfs:label ?family_name .
  ?species rdfs:subClassOf+ ?order .
  ?order taxon:rank taxon:Order .
  ?order rdfs:label ?order_name .
}
ORDER BY ?order_name ?family_name ?species_name
