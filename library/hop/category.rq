#!/usr/bin/env spang2
# @endpoint ortho
# @param geneid=100288485

PREFIX hop: <http://purl.org/net/orthordf/hOP/ontology#>

SELECT ?grp ?max ?cat
WHERE {
  {
    SELECT ?grp (max(?time) as ?max)
    WHERE {
      ?grp orth:member ncbigene:{{geneid}} .
      ?grp orth:organism ?org .
      ?org hop:branchTimeMya ?time .
    }
  }

  BIND(IF(?max<180, "M",
       IF(?max<430, "V",
       IF(?max<735, "LT",
       IF(?max<750, "EH",
       IF(?max<850, "A",
       IF(?max<940, "C",
       IF(?max<1000, "CF",
       IF(?max<1400, "F",
       IF(?max<1520, "P",
       IF(?max<1660, "AB",
          "O"))))))))))
       AS ?cat)
}
